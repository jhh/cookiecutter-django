APPS_DIR="{{ cookiecutter.project_slug }}"
{% if cookiecutter.use_htmx == 'y' %}
USE_HTMX=Y
{% endif %}
{% if cookiecutter.use_hyperscript == 'y' %}
USE_HYPERSCRIPT=Y
{% endif %}

RED='\033[1;31m'
GREEN='\033[1;32m'
NC='\033[0m' # No Color

_message() {
    echo "${GREEN}$1${NC}"
}

_warn() {
    echo "${RED}$1${NC}"
}